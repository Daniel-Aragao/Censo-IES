{
    "censo_databases": {
        "tables": ["aluno", "curso", "ies"],
        "types":{
            "char": "TEXT",
            "num": "DECIMAL(15,3)"
        }
    },
    "dictionary": {
        "sheets": {
            "DM_ALUNO": "ALUNO",
            "DM_CURSO": "CURSO",
            "DM_IES": "IES"
        },
        "columns": {
            "ord": {"name": "ORD", "mandatory": true},
            "name": {"name": "NOME DA VARIÁVEL", "mandatory": true},
            "description": {"name": "DESCRIÇÃO DA VARIÁVEL", "mandatory": true},
            "type": {"name": "TIPO", "mandatory": true},
            "size": {"name": "TAM", "mandatory": false},
            "category": {"name":"DESCRIÇÃO DAS CATEGORIAS", "mandatory": false},
            "obs": {"name": "OBSERVAÇÃO", "mandatory": false}
        },
        "header_line": 1
    },
    "data_csv_file": {
      "delimiter": "|",
      "encoding": "Windows 1252",
      "encoding2": "iso-8859-14",
      "lines_limit": 0,
      "bulk_limit": 10000
    },
    "database_access": {
        "name": "diplan_censo",
        "user": "diplan",
        "password": "diplan",
        "host": "127.0.0.1"
    }
}
